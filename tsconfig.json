{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*.ts",
    "src/**/*.astro",
    "src/**/*.d.ts"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@vue/*": [
        "src/components/vue/*"
      ],
      "@types": [
        "src/env.d.ts"
      ],
      "@stores/*": [
        "src/stores/*"
      ]
    },
    "jsxImportSource": "react",
  }
}